<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yytxdy.fim.server.mapper.MessageMapper">
    <insert id="save">
        INSERT INTO `t_message` (
            `message_id`,
            `sender_id`,
            `receiver_id`,
            `message_type`,
            `content`
        ) values (#{messageId},#{senderId},#{receiverId},#{messageType},#{content})
    </insert>
</mapper>
